  author = {Fei, Jimin and Hong, Angela and Dobbins, Timothy A. and Jones, Deanna and Lee, C. Soon and Loo, Christine and {Al-Ghamdi}, Mohammad and Harnett, Gerald B. and Clark, Jonathan and O'Brien, Christopher J. and Rose, Barbara},
  year = {2009},
