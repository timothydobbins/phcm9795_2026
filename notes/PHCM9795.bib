@article{sileo_etal23,
  title = {Gender {{Role Discrepancy Stress}} and {{COVID-19 Prevention Behaviors Among Men}} in the {{United States}}},
  author = {Sileo, Katelyn M. and Luttinen, Rebecca and Mu{\~n}oz, Suyapa and Hill, Terrence D.},
  year = {2023},
