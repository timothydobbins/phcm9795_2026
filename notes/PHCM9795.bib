  author = {Blanch, Bianca and Degenhardt, Louisa and Buckley, Nicholas A. and Gisev, Natasa and Dobbins, Timothy and Karanges, Emily A. and Larance, Briony and Larney, Sarah and Pearson, Sallie-Anne},
