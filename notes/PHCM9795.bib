@article{brettPsychotropicPolypharmacyAustralia2017,
  title = {Psychotropic Polypharmacy in {{Australia}}, 2006 to 2015: A Descriptive Cohort Study},
  author = {Brett, Jonathan and Daniels, Benjamin and Karanges, Emily A. and Buckley, Nicholas A. and Schneider, Carl and Nassir, Atheer and McLachlan, Andrew J. and Pearson, Sallie-Anne},
