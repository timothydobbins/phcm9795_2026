@article{althubaiti_althubaiti24,
  title = {Flipping the {{Online Classroom}} to {{Teach Statistical Data Analysis Software}}: {{A Quasi-Experimental Study}}},
  shorttitle = {Flipping the {{Online Classroom}} to {{Teach Statistical Data Analysis Software}}},
  author = {Althubaiti, Alaa and Althubaiti, Suha M.},
  year = {2024},
