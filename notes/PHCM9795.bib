@article{ilsarRelationshipPulmonaryArtery2010b,
  title = {The {{Relationship Between Pulmonary Artery}} and {{Pulmonary Capillary Wedge Pressure}} for the {{Diagnosis}} of {{Pulmonary Vascular Disease}}},
