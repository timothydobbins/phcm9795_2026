  keywords = {Amphetamine-Related Disorders,amphetamine-type stimulants,Australia,Crime,dependence,Hospitalization,Hotlines,Humans,injecting,Linear Models,methamphetamine,Methamphetamine,N-Methyl-34-methylenedioxyamphetamine,psychosis,Street Drugs,Surveys and Questionnaires},
