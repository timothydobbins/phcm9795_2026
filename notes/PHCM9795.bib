@article{brehautValidationDecisionRegret2003,
  title = {Validation of a {{Decision Regret Scale}}},
  author = {Brehaut, Jamie C. and O'Connor, Annette M. and Wood, Timothy J. and Hack, Thomas F. and Siminoff, Laura and Gordon, Elisa and {Feldman-Stewart}, Deb},
